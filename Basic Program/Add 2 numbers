public class AddTwoNumbers {
    public static void main(String[] args) {
        // Declare two numbers
        int num1 = 10; // First number
        int num2 = 20; // Second number

        // Calculate the sum
        int sum = num1 + num2;

        // Display the result
        System.out.println("The sum of " + num1 + " and " + num2 + " is " + sum);
    }
}

